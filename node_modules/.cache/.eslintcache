signOut(auth)\r\n      .then(() => {\r\n        console.log(\"sign out successful\");\r\n      })\r\n      .catch((error) => console.log(error));\r\n  }; */\r\n\r\n  return (\r\n    <div className=\"Login\">\r\n      <div className=\"auth-form-container\">\r\n        <form className=\"login-form\" onSubmit={signIn}>\r\n          <h1>Sign in</h1>\r\n          <label className=\"input\">\r\n            <input\r\n              className=\"input__field\"\r\n              type=\"email\"\r\n              placeholder=\" \"\r\n              value={email}\r\n              onChange={(e) => setEmail(e.target.value)}\r\n            />\r\n            <span className=\"input__label\">Enter your email</span>\r\n          </label>\r\n\r\n          <label className=\"input\">\r\n            <input\r\n              className=\"input__field\"\r\n              type=\"password\"\r\n              placeholder=\" \"\r\n              value={password}\r\n              onChange={(e) => setPassword(e.target.value)}\r\n            />\r\n            <span className=\"input__label\">Enter your password</span>\r\n          </label>\r\n\r\n          <button type=\"submit\">Sign in</button>\r\n\r\n          <div\r\n            className=\"link-btn\"\r\n            onClick={navigateToSignUp}\r\n            data-replace=\"Sign up\"\r\n          >\r\n            <span>Don't have an account?</span>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default Login;\r\n","C:\\Users\\admin\\Desktop\\BTL\\CNPM2024\\src\\components\\Signup.js",[],"C:\\Users\\admin\\Desktop\\BTL\\CNPM2024\\src\\context\\index.js",[],"C:\\Users\\admin\\Desktop\\BTL\\CNPM2024\\src\\components\\User.js",["119"],"import React from \"react\";\r\nimport logo from \"../images/ghould.png\";\r\nimport { auth } from \"../firebase/FirebaseForLogin\";\r\nimport { onAuthStateChanged, signOut } from \"firebase/auth\";\r\nimport { useNavigate } from \"react-router-dom\";\r\nimport { BoxArrowRight } from \"react-bootstrap-icons\";\r\n\r\nfunction User() {\r\n  const navigate = useNavigate();\r\n  const userSignOut = () => {\r\n    signOut(auth)\r\n      .then((userCredential) => {\r\n        navigate(\"/\");\r\n      })\r\n      .catch((error) => console.log(error));\r\n  };\r\n\r\n  return (\r\n    <div className=\"User\">\r\n      <div className=\"logo\">\r\n        <img src={logo} alt=\"logo\" />\r\n      </div>\r\n      <div className=\"info\">\r\n        <p>Hanoi Ghoul</p>\r\n        <button onClick={userSignOut}>\r\n          <BoxArrowRight size={20} />\r\n          <div className=\"textOut\">Sign out</div>\r\n        </button>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default User;\r\n","C:\\Users\\admin\\Desktop\\BTL\\CNPM2024\\src\\components\\Calendar.js",["120","121"],"import React, { useContext } from \"react\";\r\nimport { CalendarDate, CaretUp } from \"react-bootstrap-icons\";\r\nimport { calendarItems } from \"../constants\";\r\nimport { TodoContext } from \"../context\";\r\nimport { FaTasks } from \"react-icons/fa\";\r\nimport { MdDoubleArrow } from \"react-icons/md\";\r\nimport { MdDoneAll } from \"react-icons/md\";\r\nfunction Calendar() {\r\n  // CONTEXT\r\n  const { setSelectedProject } = useContext(TodoContext);\r\n\r\n  return (\r\n    <div className=\"Calendar\">\r\n      <div className=\"header\">\r\n        <div className=\"title\">\r\n          <p>Tasks</p>\r\n        </div>\r\n        {/* <div className=\"btns\">\r\n          <span>\r\n            <CaretUp size=\"20\" />\r\n          </span>\r\n        </div> */}\r\n      </div>\r\n      <div className=\"items\">\r\n        <div\r\n          className=\"item\"\r\n          onClick={() => setSelectedProject(calendarItems[0])}\r\n        >\r\n          <FaTasks size={14} />\r\n          <p>{calendarItems[0]}</p>\r\n        </div>\r\n        <div\r\n          className=\"item\"\r\n          onClick={() => setSelectedProject(calendarItems[1])}\r\n        >\r\n          <