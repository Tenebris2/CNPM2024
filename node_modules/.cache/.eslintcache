alDay);\r\n  const [time, setTime] = useState(initialTime);\r\n  const [todoProject, setTodoProject] = useState(initialTodoProject);\r\n\r\n  // State to track form data changes\r\n  const [formData, setFormData] = useState({\r\n    text: initialText,\r\n    day: initialDay,\r\n    time: initialTime,\r\n    todoProject: initialTodoProject,\r\n  });\r\n\r\n  // Update formData state when any form field changes\r\n  useEffect(() => {\r\n    setFormData({\r\n      text,\r\n      day,\r\n      time,\r\n      todoProject,\r\n    });\r\n  }, [text, day, time, todoProject]);\r\n\r\n  return (\r\n    <MuiPickersUtilsProvider utils={DateFnsUtils}>\r\n      <form\r\n        onSubmit={(e) => {\r\n          e.preventDefault();\r\n          handleSubmit(formData);\r\n        }}\r\n        className=\"EditTodoForm\"\r\n      >\r\n        <div className=\"text\">\r\n          {heading && <h3>{heading}</h3>}\r\n          <input\r\n            spellCheck=\"false\"\r\n            type=\"text\"\r\n            value={text}\r\n            onChange={(e) => setText(e.target.value)}\r\n            placeholder=\"Task name\"\r\n            autoFocus\r\n          />\r\n        </div>\r\n        <div className=\"pick-day\">\r\n          <div className=\"title\">\r\n            <p>Due date</p>\r\n            <DatePicker\r\n              value={day}\r\n              onChange={(day) => setDay(day)}\r\n              format=\"dd-MM-yy\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div className=\"pick-time\">\r\n          <div className=\"title\">\r\n            <p>Choose time</p>\r\n            <TimePicker value={time} onChange={(time) => setTime(time)} />\r\n          </div>\r\n        </div>\r\n        <div className=\"pick-project\">\r\n          <div className=\"title\">\r\n            <p>List</p>\r\n            <div className=\"projects\">\r\n              {projects.length > 0 ? (\r\n                <select\r\n                  className=\"selectList\"\r\n              